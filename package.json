{
  "name": "cueprise-website",
  "version": "1.3.0",
  "private": true,
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --clearMocks --watchAll=false --detectOpenHandles",
    "test:cover": "npm test -- --coverage",
    "eject": "react-scripts eject",
    "docker:build": "DOCKER_BUILDKIT=1 docker build --secret id=npmrc,src=${HOME}/.npmrc -t cuesoftcloud/cueprise-website .",
    "predocker:run": "npm run docker:build",
    "docker:run": "docker run --name cueprise-website --env-file .env -p 80:80 -d cuesoftcloud/cueprise-website",
    "docker:start": "docker-compose up --build -d",
    "docker:stop": "docker-compose down",
    "predocker:test": "npm run docker:build -- --target build-stage",
    "docker:test": "docker-compose run cueprise-website npm run test:cover",
    "postdocker:test": "npm run docker:stop",
    "predocker:watch": "npm run docker:build -- --target build-stage",
    "docker:watch": "docker-compose run -v ${PWD}/src:/app/src -p 3000:3000 cueprise-website npm start || npm run docker:stop",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "prettier \"**/*.ts\" \"**/*.js\" \"**/*.tsx\" \"**/*.html\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "postinstall": "npx patch-package",
    "prepare": "husky install || true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CuesoftCloud/cueprise-website.git"
  },
  "keywords": [],
  "author": "",
  "bugs": {
    "url": "https://github.com/CuesoftCloud/cueprise-website/issues"
  },
  "homepage": "https://cueprise.com",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": "eslint --cache --fix",
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,html,md}": "prettier --write"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@cuesoftcloud/logger": "^1.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.4.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "eslint-config-prettier": "^8.6.0",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "react-scripts": "5.0.1",
    "setimmediate": "^1.0.5",
    "tailwindcss": "^3.2.4",
    "web-vitals": "^3.1.1",
    "winston-react": "^1.0.0-RC.0"
  },
  "devDependencies": {
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.11.18",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.33.0",
    "eslint-config-react-app": "^7.0.1",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "nth-check": "^2.1.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "typescript": "^4.9.4"
  }
}
